            "preAttachCommands": [
                "mon speed 8000"
            ],
            "request": "attach",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "device": "NRF52832_XXAA",
            "interface": "swd",
            // "serialNumber": "", //If you have more than one J-Link probe, add the serial number here.
            // "jlinkscript":"${workspaceFolder}/xxx.jlink",
